Here are all records:
<style>
    table, th, td {
    border: 1px solid black;
    }
</style>

<table style="width:100%">

    <% people.forEach( (person) => { %>
        <tr>
        <td><a href="/person?name=<%=person.name%>"> 
        <%= person.name %></a>:</td>
        <td><%= person.city %></td>   
        <td><%= person.phonenumber %></td>
        <td><%= person.website %></td>
        <td><%= person.about %></td>
        <td><a href="/delete?name=<%=person.name%>"><button>Delete</button></a> </td>
        </tr>
        
   <% }) %>  

</table> 

